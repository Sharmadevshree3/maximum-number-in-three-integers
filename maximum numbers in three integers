# maximum-number-in-three-integers
#include <stdio.h>

int main()
{
    int a,b,c;
    printf("enter the value of a, b and c:");
    scanf("%d%d%d",&a,&b,&c);
    if(a>b)
    {
        
        if(a>c)
        {
        printf("maximum number is a");
        }
        else
        {
        printf("maximum number is c");
        }
    }
    else
    {
        
        if(b>c)
        {
        printf("maximum number is b");
        }
        else
  {
        printf("maximum number is c");
        }
    }
    return 0;
}
